{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TenVidDb": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=TenVidDb;Integrated Security=True;Pooling=False;Encrypt=True;Trust Server Certificate=True"
  },
  "FileUpload": {

    "ImageMaxSizeInMB": 5,
    "ImageContentTypes": [
      "image/jpeg",
      "image/jpg",
      "image/png",
      "image/x-png"
    ],
    "VideoMaxSizeInMB": 100,
    "VideoContentTypes": [
      "video/mp4",
      "video/mpeg",
      "video/quicktime",
      "video/x-msvideo",
      "video/3gpp",
      "video/x-flv"
    ]
  }

}


